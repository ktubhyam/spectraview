{
  "name": "spectraview",
  "version": "0.1.1",
  "description": "Interactive React component for vibrational spectroscopy (IR, Raman, NIR)",
  "author": "Tubhyam Karthikeyan <takarthikeyan25@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktubhyam/spectraview.git"
  },
  "homepage": "https://github.com/ktubhyam/spectraview",
  "bugs": {
    "url": "https://github.com/ktubhyam/spectraview/issues"
  },
  "keywords": [
    "react",
    "spectroscopy",
    "infrared",
    "raman",
    "NIR",
    "spectrum",
    "viewer",
    "visualization",
    "JCAMP-DX",
    "chemistry",
    "scientific"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "jcampconverter": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "jcampconverter": {
      "optional": true
    }
  },
  "dependencies": {
    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/d3-array": "^3.2.1",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-selection": "^3.0.10",
    "@types/d3-transition": "^3.0.9",
    "@types/d3-zoom": "^3.0.8",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "jsdom": "^25.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0",
    "storybook": "^10.2.12",
    "@storybook/react-vite": "^10.2.12",
    "@storybook/addon-a11y": "^10.2.12",
    "@storybook/addon-docs": "^10.2.12"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run test:run && npm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "ci:publish": "npm run build && changeset publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "publishConfig": {
    "access": "public"
  }
}
